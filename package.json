    "build": "node-gyp build",
    "install": "node-gyp rebuild",
    "concourse-build-electron": "make webpack",
    "concourse-test": "npm test",
    "concourse-test-electron": "npm test",
    "concourse-test-node-cli": "make lint test-cli sanity-checks"

{
  "name": "etcher",
  "displayName": "Etcher",
  "version": "1.0.0-beta.9",
  "main": "lib/start.js",
  "description": "An image flasher with support for Windows, OS X and GNU/Linux.",
  "homepage": "https://github.com/resin-io/etcher",
  "engines": {
    "node": "6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io/etcher.git"
  },
  "scripts": {
    "test": "gulp lint && electron-mocha --recursive --renderer tests/gui -R min",
    "start": "electron lib/start.js",
    "shrinkwrap": "node ./scripts/shrinkwrap.js"
  },
  "author": "Juan Cruz Viotti <juan@resin.io>",
  "license": "Apache-2.0",
  "copyright": "Copyright 2016 Resinio Ltd",
  "packageIgnore": [
    "LICENSE",
    "Makefile",
    "(.*)\\.pem",
    "(.*)\\.p12",
    "(.*)\\.log",
    "assets/osx",
    "appveyor.yml",
    "bower.json",
    "gulpfile.js",
    "screenshot.png",
    "tests",
    "etcher-release",
    "lib/scss",
    "bin/etcher"
  ],
  "shrinkwrapIgnore": [
    "macos-alias",
    "fs-xattr"
  ],
  "builder": {
    "win": {
      "title": "Etcher",
      "version": "v1.0.0-beta.9",
      "publisher": "Resin.io",
      "icon": "assets/icon.ico",
      "verbosity": 1
    }
  },
  "optionalDependencies": {
    "elevator": "^2.1.0",
    "removedrive": "^1.0.0"
  },
  "dependencies": {
    "angular": "^1.5.3",
    "angular-if-state": "^1.0.0",
    "angular-middle-ellipses": "^1.0.0",
    "angular-moment": "^1.0.0-beta.6",
    "angular-ui-bootstrap": "^1.3.2",
    "angular-ui-router": "^0.2.18",
    "bluebird": "^3.0.5",
    "bootstrap-sass": "^3.3.5",
    "chalk": "^1.1.3",
    "drivelist": "^3.2.0",
    "electron-is-running-in-asar": "^1.0.0",
    "etcher-image-stream": "^2.2.0",
    "etcher-image-write": "^5.0.2",
    "file-tail": "^0.3.0",
    "flexboxgrid": "^6.3.0",
    "immutable": "^3.8.1",
    "is-elevated": "^1.0.0",
    "lodash": "^4.5.1",
    "redux": "^3.5.2",
    "redux-localstorage": "^0.4.1",
    "resin-cli-errors": "^1.2.0",
    "resin-cli-form": "^1.4.1",
    "resin-cli-visuals": "^1.2.8",
    "rx": "^4.1.0",
    "semver": "^5.1.0",
    "sudo-prompt": "^5.0.3",
    "tail": "^1.1.0",
    "tmp": "0.0.28",
    "trackjs": "^2.1.16",
    "umount": "^1.1.3",
    "username": "^2.1.0",
    "xml2js": "^0.4.16",
    "yargs": "^4.6.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.7",
    "browserify": "^13.0.1",
    "cz-conventional-changelog": "^1.1.6",
    "electron-builder": "^2.6.0",
    "electron-mocha": "^1.2.2",
    "electron-osx-sign": "^0.3.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "1.1.1",
    "gulp": "^3.9.0",
    "gulp-jscs": "^3.0.2",
    "gulp-jshint": "^2.0.0",
    "gulp-sass": "^2.0.4",
    "gulp-sequence": "^0.4.5",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.0.1",
    "jsonfile": "^2.3.1",
    "mochainon": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

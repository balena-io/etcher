    "start": "electron lib/start.js",
    "postshrinkwrap": "node ./scripts/clean-shrinkwrap.js",
    "configure": "node-gyp configure",
    "build": "node-gyp build",
    "install": "node-gyp rebuild"
  },
  "author": "Resin Inc. <hello@etcher.io>",

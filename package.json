    "redux": "4.0.5",
    "rendition": "19.2.0",
    "resin-corvus": "2.0.5",
    "semver": "7.3.2",
    "styled-components": "5.1.0",
    "sudo-prompt": "github:zvin/sudo-prompt#7cdede2f0da28fbcc2db48402d7d935f3a825c91",
    "sys-class-rgb-led": "3.0.0",
    "url-loader": "4.1.1",
    "uuid": "8.1.0"
  },

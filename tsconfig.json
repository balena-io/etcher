{
	"compilerOptions": {
		"strict": true,
		"target": "es2019",
		"typeRoots": ["./node_modules/@types", "./typings"],
		"module": "commonjs",
		"lib": ["dom", "esnext"],
		"declaration": true,
		"declarationMap": true,
		"jsx": "react",
		"pretty": true,
		"sourceMap": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true
	}
}
